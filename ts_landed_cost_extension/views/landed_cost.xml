<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id='view_stock_landed_cost_form_custom' model='ir.ui.view'>
        <field name="name">stock.landed.cost.form.custom</field>
        <field name="model">stock.landed.cost</field>
        <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='valuation_adjustment_lines']/tree/field[@name='final_cost']" position="after">
                <field name="is_included"/>
            </xpath>
        </field>
    </record>

</odoo>